  "compilerOptions": {
    "target": "es5",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "ESNext",
    "lib": ["ESNext"],
    "noImplicitAny": true,
    "allowJs": true,
    "isolatedModules": true,
    "noEmit": true,
    "outDir": "./dist/",
  }
